<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/maskFilter"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="MaskFilter"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"/>
    <Button
        android:id="@+id/a2xfermodeView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="porterDiffuxfermodeView"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toEndOf="@id/maskFilter"/>
    <Button
        android:id="@+id/a3CircleImageView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="porterDiffuxfermodeView"
        app:layout_constraintTop_toBottomOf="@id/maskFilter"
        app:layout_constraintStart_toStartOf="parent"/>


</androidx.constraintlayout.widget.ConstraintLayout>